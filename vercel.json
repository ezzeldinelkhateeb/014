{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth-check",
      "dest": "/api/auth-check.js"
    },
    {
      "src": "/api/debug-routes",
      "dest": "/api/debug-routes.js"
    },
    {
      "src": "/api/sheets/update-bunny-embeds",
      "dest": "/api/sheets/update-bunny-embeds.js"
    },
    {
      "src": "/api/sheets/update-final-minutes",
      "dest": "/api/sheets/update-final-minutes.js"
    },
    {
      "src": "/api/proxy/create-video",
      "dest": "/pages/api/proxy/create-video.js"
    },
    {
      "src": "/api/proxy/base/([^/]+.*)",
      "dest": "/pages/api/proxy/base/[...path].js?path=$1"
    },
    {
      "src": "/api/proxy/video/library/([^/]+)/collections",
      "dest": "/pages/api/proxy/video/library/[libraryId]/collections.js?libraryId=$1"
    },
    {
      "src": "/api/proxy/video/([^/]+.*)",
      "dest": "/pages/api/proxy/video/[...path].js?path=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
