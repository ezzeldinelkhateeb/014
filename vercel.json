{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/auth-check",
      "dest": "/api/auth-check.js"
    },
    {
      "src": "/api/proxy/base/videolibrary",
      "dest": "/api/proxy/base/videolibrary.js"
    },
    {
      "src": "/api/proxy/base/([^/]+)",
      "dest": "/api/proxy/base/[...path].js?path=$1"
    },
    {
      "src": "/api/proxy/base/([^/]+)/([^/]+)",
      "dest": "/api/proxy/base/[...path].js?path=$1/$2"
    },
    {
      "src": "/api/proxy/video/library/([^/]+)/collections",
      "dest": "/api/proxy/video/library/[libraryId]/collections.js?libraryId=$1"
    },
    {
      "src": "/api/proxy/video/([^/]+)/([^/]+)/([^/]+)",
      "dest": "/api/proxy/video/[...path].js?path=$1/$2/$3"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
