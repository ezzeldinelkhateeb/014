{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/proxy/base/(.*)",
      "dest": "/api/proxy/base/$1"
    },
    {
      "src": "/api/proxy/video/(.*)",
      "dest": "/api/proxy/video/$1"
    },
    {
      "src": "/api/sheets/(.*)",
      "dest": "/api/sheets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type,Authorization,AccessKey,x-api-key" }
      ]
    }
  ]
}
