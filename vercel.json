{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization, AccessKey" }
      ]
    }
  ],
  "env": {
    "VITE_BUNNY_API_KEY": "@vite_bunny_api_key",
    "GOOGLE_SHEETS_SPREADSHEET_ID": "@google_sheets_spreadsheet_id",
    "GOOGLE_SHEETS_CREDENTIALS_JSON": "@google_sheets_credentials_json",
    "GOOGLE_SHEET_NAME": "@google_sheet_name"
  }
}
  }
}
      "src": "/api/proxy/base/([^/]+)/([^/]+)",
      "dest": "/api/proxy/base/[...path].js?path=$1/$2"
    },
    {
      "src": "/api/proxy/video/([^/]+)/([^/]+)/([^/]+)",
      "dest": "/api/proxy/video/[...path].js?path=$1/$2/$3"
    },
    {
      "src": "/api/proxy/video/library/([^/]+)/collections",
      "dest": "/api/proxy/video/library/[libraryId]/collections.js?libraryId=$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization, AccessKey, access-key, x-api-key" }
      ]
    }
  ]
}
